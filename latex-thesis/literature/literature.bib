 journal = {Systems},
 doi = {10.3390/systems11030123},
 file = {Jansen, Meisen et al. 2023 - Stop Guessing in the Dark:Attachments/Jansen, Meisen et al. 2023 - Stop Guessing in the Dark.pdf:application/pdf}
