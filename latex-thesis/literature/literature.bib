 keywords = {Asset Administration Shell;Atomic absorption spectrometry;Circular economy;Digital Lifecycle Passport;Digital Product Passport;Digital products;End of life managements;End-of-life managements;Information dissemination;Life cycle;Life cycle assessment;Product life cycles;Recyclers},
 pages = {831--836},
